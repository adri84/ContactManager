<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>

<ToolBar fx:id="mainToolbar" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1">
   <items>
      <Button mnemonicParsing="false" onAction="#addContact" text="New Contact" />
      <Button mnemonicParsing="false" text="Edit Contact" />
      <Button mnemonicParsing="false" text="Remove Contact" />
      <Separator orientation="VERTICAL">
         <padding>
            <Insets left="25.0" right="25.0" />
         </padding>
      </Separator>
      <TextField alignment="CENTER" prefHeight="27.0" prefWidth="167.0" />
      <Button alignment="CENTER_RIGHT" cache="true" contentDisplay="TEXT_ONLY" mnemonicParsing="false" text="?" textAlignment="CENTER" />
   </items>
</ToolBar>
